#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhenispir/Documents/GitHub/my-swyxkit/node_modules/.pnpm/histoire@0.17.17_vite@5.4.1/node_modules/histoire/node_modules:/Users/zhenispir/Documents/GitHub/my-swyxkit/node_modules/.pnpm/histoire@0.17.17_vite@5.4.1/node_modules:/Users/zhenispir/Documents/GitHub/my-swyxkit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhenispir/Documents/GitHub/my-swyxkit/node_modules/.pnpm/histoire@0.17.17_vite@5.4.1/node_modules/histoire/node_modules:/Users/zhenispir/Documents/GitHub/my-swyxkit/node_modules/.pnpm/histoire@0.17.17_vite@5.4.1/node_modules:/Users/zhenispir/Documents/GitHub/my-swyxkit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../histoire@0.17.17_vite@5.4.1/node_modules/histoire/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../../histoire@0.17.17_vite@5.4.1/node_modules/histoire/bin.mjs" "$@"
fi
